{
  "name": "microbloggingBackend",
  "version": "1.0.0",
  "description": "Backend for microblogging",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon --config nodemon.json server/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' server/index.ts | pino-pretty",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "ejs": "^3.1.6",
    "errorhandler": "^1.5.1",
    "express": "^4.16.4",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.2",
    "http-status-codes": "^1.3.0",
    "jsonwebtoken": "^8.5.1",
    "methods": "^1.1.2",
    "mongoose": "^5.3.11",
    "mongoose-unique-validator": "^2.0.3",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "pino": "^5.8.1",
    "request": "^2.88.2",
    "slug": "^5.0.1",
    "swagger-express-middleware": "^1.1.1",
    "underscore": "^1.13.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.24",
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/mongoose": "^5.3.1",
    "@types/pino": "^5.20.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "nodemon": "^1.18.6",
    "pino-pretty": "^2.2.3",
    "shelljs": "^0.8.3",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
